/* components/kf/kf.wxss */
.box {
  padding:15rpx 28rpx 15rpx;
  background:rgba(27,27,27,.3);
  border-radius:40rpx 0 0 40rpx;
  position: fixed;
  top:40%;
  right:0;
}
.inner {
  width:168rpx !important;
  height:160rpx !important;
  background:rgba(255,255,255,1);
  border-radius:30rpx;
  font-size:18rpx;
  color:rgba(27,27,27,1);
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding:0 !important;
  margin:0 !important;
  border:none !important;
}
.inner image {
  width:44rpx;
  height:44rpx;
  margin-bottom:22rpx;
}