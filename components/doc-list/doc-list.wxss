/* components/doc-list/doc-list.wxss */
.docbox {
  height:200rpx;
  background: #fff;
}
.docbox .ss {
  display: flex;
  margin: 0 25rpx;
  width:100%;
  height:100%;
}
.item {
  width:120rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  color:#1B1B1B;
  font-size:21rpx;
  margin:0 25px;
  justify-content: center;
}
.item image {
  width:110rpx;
  height:110rpx;
  border-radius: 50%;
  margin-bottom: 16rpx;
}
.item:last-child {
  padding-right:50rpx;
}