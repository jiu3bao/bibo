}
.ta {
  border-bottom:1px solid rgba(229,229,229,1);
  font-size:25rpx;
  height: 190rpx;
  padding:33rpx 45rpx;
  box-sizing: border-box;
}
.photos view {
  width:120rpx;
  text-align: center;
  color: #313131;
  font-size: 25rpx; 
}
.photos image {
  width: 100%;
  height:120rpx;
  background:rgba(238,238,238,1);
  border:1px solid rgba(220,220,220,1);
  border-radius:10rpx;
  margin-top:20rpx;
}