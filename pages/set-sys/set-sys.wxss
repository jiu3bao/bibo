page{
  background: #F7F7F7;
}

.o {
  margin-top:40rpx;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  color:#EC5B43;
  height:100rpx;
  font-size:33rpx;
  border-top:1px solid rgba(181,181,181,1);
  border-bottom:1px solid rgba(181,181,181,1);
}
.o image {
  width:38rpx;
  height:38rpx;
  margin-right:25rpx
}
.tip {
  position: fixed;
  width:380rpx;
  height:165rpx;
  bottom:158rpx;
  left:50%;
  margin-left:-190rpx;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color:#1B1B1B;
  font-size:25rpx;
}