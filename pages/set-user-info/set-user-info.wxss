/* pages/set-user-info/set-user-info.wxss */
.contain {
  height:100%;
  box-sizing:border-box;
  background:#f5f5f5;
}
.head {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height:162rpx;
  margin-top:20rpx;
  background:#fff;
  padding:0 50rpx;
}
.head image {
  width:124rpx;
  height:124rpx;
  border-radius: 50%;
}
.head view {
  font-size:25rpx;
  color:#626262;
}
.radio {
  display: flex;
  align-items: center;
  height:90rpx;
  padding-left:70rpx;
}
.radio radio {
  font-size:25rpx;
  color:#626262;
  height:30rpx;
  margin-right:40rpx;
}
.info {
  background:#fff;
}
.info>view {
  display: flex;
  height:100rpx;
  padding:0 50rpx;
  justify-content: space-between;
  color:#626262;
  font-size:25rpx;
  align-items: center;
  border-bottom:1rpx solid #d6d6d6
}
input {
  text-align: right
}
button {
  height:80rpx;
  width:560rpx;
  color:#fff;
  border-radius: 40rpx;
  background:linear-gradient(to right, #69dec1 , #44cbed);
  margin-top:40rpx;
}