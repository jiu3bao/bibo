/* pages/member-manage/member-manage.wxss */
.contain {
  height:100%;
  box-sizing: border-box;
}
.tab {
  display: flex;
}
.tab view {
  width:50%
}
.tab .ac {
  background:blue;
  color:#fff
}
.head {
  display: flex;
  justify-content: space-between
}
.swiper {
  height:100%;
}
.swiper-item {
  height:100%   
}
.item {
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.item > view {
  display: flex;
  align-items: center;
}
.item image {
  width:20rpx;
  height:20rpx;
  border-radius: 50%
}