/* pages/extension-service/extension-service.wxss */
page {
  background: #f5f5f5;
}
.contain {
}
.box {
  padding-left:30rpx;
  padding-right:30rpx;
  box-sizing: border-box;
}
.tab {
  display: flex;
  border-top:1rpx solid #e6e6e6;
  background: #fff;
}
.tab view {
  width:50%;
  text-align:center;
  height:66rpx;
  line-height:66rpx;
  color:#1b1b1b;
  font-size:25rpx;
}
.tab .ac {
  background:#229aff;
  color:#fff
}
.list {
  width:100%;
  position: relative;
  margin:20rpx auto;
  border:1rpx solid #e6e6e6;
  background: #fff;
  padding:30rpx 20rpx;
  color:#313131;
  font-size:24rpx;
  box-sizing: border-box;
  border-radius: 5rpx;
}
.dot {
  width:40rpx;
  height:40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 50%;
  color:#fff;
  background:#ff0000;
  position: absolute;
  top:-20rpx;
  right:-20rpx;
}
.title {
  color:#5c629e;
  font-size:28rpx;
}
.done {
  display: absolute;
  right:0;
  width:60rpx;
  top:30rpx;
  color:#fff;
  padding:10rpx 20rpx;
  background:#999;
  border-radius: 5rpx;
}