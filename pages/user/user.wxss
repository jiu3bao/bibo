/* pages/user/user.wxss */
page {
  background:#f7f7f7;
  height:auto;
}
.bg {
  height:352rpx;
  position: absolute;
  top:0;
  left:0;
  width: 100%;
}
.contain {
  margin-top:98rpx;
  width: 100%;
}
swiper-item {
  width:86% !important;
  display: flex;
  justify-content: center;
  margin:0 7%;
  padding:0;
}
.ff {
  width:96%;
}
.mycontent {
  display: flex;
  background: #fff;
  margin-top:50rpx;
  flex-wrap: wrap;
  border:1px solid rgba(220,220,220,1);
}
.mycontent .item {
  width:25%;
  text-align: center;
  color:#707070;
  font-size:20rpx;
  flex-shrink: 0;
  padding: 40rpx 0;
}
.item  image {
  width:48rpx;
  height:48rpx;
  margin-bottom: 20rpx;
}
.kf {
  position: fixed;
  width:666rpx;
  height:200rpx;
  bottom:22rpx;
  left:50%;
  margin-left:-333rpx;

}
.kf .bg {
  width:100%;
  height:100%;
}
.kf .c {
  position: absolute;
  width: 100%;
  height:100%;
  top:0;
  left:0;
  display: flex;
  align-items: center;
}
.c image {
  width: 105rpx;
  height:105rpx;
  margin:0 30rpx 0 47rpx;
  border-radius: 50%;
}
.c .right {
  font-size:21rpx;
  color:rgba(27,27,27,1);
  flex-direction: column;
}
.right view:last-child {
  margin-top:19rpx;
}