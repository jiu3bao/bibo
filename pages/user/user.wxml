<!--pages/user/user.wxml-->
<view>
  <image src='/img/bg.png' class='bg'></image>
  <swiper class="contain">
    <block class='s-item'>
      <swiper-item>
        <UserheadInfo></UserheadInfo>
      </swiper-item>
    </block>
    <block>
      <swiper-item class='wallet'>
        <Userhead></Userhead>
          
      </swiper-item>
    </block>
    
  </swiper>
  <view class='mycontent'>
    <view wx:for='{{list}}' class='item' bindtap="route2" data-route='{{item.route}}'>
      <image src='{{item.icon}}'></image>
      <view>{{item.name}}</view>
    </view>
  </view>
  <view class='kf'>
    
  </view>
</view>