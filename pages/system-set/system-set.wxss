/* pages/system-set/system-set.wxss */
page {
  background: #f5f5f5;
  color:#707070;
}
.change_num {
  margin-top:30rpx;
  padding:0 30rpx;
  background: #fff;
  display: flex;
  align-items: center;
  height:100rpx;
  justify-content: space-between;
  margin-bottom:50rpx;
  font-size:24rpx;
  border-top:1rpx solid #d5d5d5;
  border-bottom:1rpx solid #d5d5d5;
}
.change_num image {
  width:40rpx;
  height:40rpx;
  transform: rotate(-90deg);
}
.log_out {
  padding:0 30rpx;
  background: #fff;
  display: flex;
  align-items: center;
  height:100rpx;
  justify-content: center;
  font-size:24rpx;
  border-top:1rpx solid #d5d5d5;
  border-bottom:1rpx solid #d5d5d5;
}
.log_out image {
  width:40rpx;
  height:40rpx;
  margin-right:30rpx;
}