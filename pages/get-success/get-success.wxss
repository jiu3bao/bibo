/* components/get-success/get-success.wxss */
.g_s {
  width:100%;
  height:100%;
  position: fixed;
  display: flex;
  flex-direction: column;
  align-items: center;
  top:0;
  left:0;
}
.bg {
  position: fixed;
  width:100%;
  height:100%;
  top:0;
  left:0;
}
.top {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  color:#1B1B1B;
  font-size: 34rpx;
  margin-bottom: 50rpx;
  margin-top:145rpx;
}
.top image {
  width:52rpx;
  height:52rpx;
  margin-right: 27rpx;
}
.code {
  position: relative;
  width:80%;
  background: #fff;
  height:258rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  color:#313131;
  font-size:75rpx;
  justify-content: center;
  margin-bottom: 110rpx;
  border:1px solid #DCDCDC;
  border-radius:10px;
}
.code .s {
  font-size:25rpx;
  color:#1B1B1B;
  margin-bottom: 42rpx;
}

.backbtn {
  position: relative;
  width:90%;
  background:linear-gradient(-90deg,rgba(59,166,156,1),rgba(98,218,147,1));
  border-radius:37px;
  color:#fff;
  font-size: 28rpx;
  line-height:75rpx;
  text-align: center;
}

