/* pages/qrcode/qrcode.wxss */
.contain {
  height:100%;
  box-sizing: border-box;
}
.canvas {
  width:100%;
  height:100%;
  position: fixed;
  top:9999999rpx;
  z-index:-1;
}
image {
  width:100%;
  height:100%
}
button {
  position: relative;
  bottom:135rpx;
  background: #229aff;
  color:#fff;
}
.pre{
  position: absolute;
  top:600rpx;
  left:50rpx;
  
}
.pre image,.next image {
  width:80rpx;
  height:80rpx;
}
.next {
  position: absolute;
  top:600rpx;
  right:50rpx;
}