/* pages/staff-page/staff-page.wxss */
page {
  font-size:26rpx;
  background: #f7f7f7
}
.search {
  width: 100%;
  height:67rpx;
  border:2px solid rgba(222, 222, 222, 1);
  border-radius:34rpx;
  display: flex;
  align-items: center;
  margin:28rpx 0;
  background: #fff;
}
.search input {
  width: 100%;
  padding-left: 40rpx;
  font-size:25rpx;
}
.search image {
  width:34rpx;
  height:34rpx;
  margin:0 36rpx;
}
.cao {
  padding:30rpx;
  padding-bottom:0;
  border-top:1rpx solid #e6e6e6;
}
.o {
  display: flex;
  margin-top:20rpx;
  justify-content: space-between;
}
.o input {
  border:none;
  text-align: left;
}
.h {
  background: #3cb5fe;
  color:#fff;
  padding:10rpx 25rpx;
  border-radius: 8rpx;
}
.err_info {
  margin-top:50rpx;
  display: flex;
  padding:30rpx 50rpx;
  align-items: center;
  background: #FFDAB9;
  color:#ff3131;
}
.err_info image {
  width:50rpx;
  height:50rpx;
  margin-right:20rpx;
}
.flex {
  display: flex;
  padding:0 30rpx;
  border-top:1rpx solid #e6e6e6;
  height:80rpx;
  align-items: center;
  background: #fff;
  color:#333;
}
.check {
  padding:0;
  color:#666;
  font-size:24rpx;
}
.between {
  justify-content: space-between
}

.disable view{
  filter: grayscale(100%);
}
.submit {
  width:60%;
  line-height:70rpx;
  margin:40rpx auto;
  color:#fff;
  text-align: center;
  background:linear-gradient(to right, #3cb5fe , #42d4fd);
  border-radius: 30rpx;
}
.submit image {
  width:100%;
  height:100%
}
.blue {
  color:#229aff
}
.red {
  color:#ff3131;
  border-top:none;
}
.icon {
  width:50rpx;
  height:50rpx;
  margin-right:10rpx;
}
.pic {
  width:120rpx;
  height:120rpx;
  margin-top:10rpx;
}
.convers-pic {
  padding:10rpx 30rpx;
  background: #fff;
  border-top:1px solid rgba(229,229,229,1);
}
.head_pic {
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  margin-right:30rpx;
}
.info {
  display: flex;
  align-items: center;
  padding-left:50rpx;
  height:160rpx;
}
.name {
  color:#1b1b1b;
  font-size:28rpx;
}
.name text {
  font-size:22rpx;
  margin-left:10rpx;
}
.phone {
  color:#707070;
  font-size:24rpx;
  margin:8rpx 0;
}
.no-border {
  border:none;
  padding:0;
  height:auto;
  font-size: 24rpx;
}
.bt-none{
  border:none;
}
.add_item{
  padding-bottom:40rpx;
  background: #fff;
}
.down {
  width:30rpx;
  height:30rpx;
  margin-left:8rpx;
}
.hos {
  display: flex;
  align-items: center
}
.tologin {
  margin-top:30rpx;
  float: right;
  margin-right:20rpx;
  font-size:33rpx;
  text-decoration: underline
}
.zx_program {
  height:76rpx;
  font-size:25rpx;
  color:#434343;
  display: flex;
  justify-content: space-around;
  background: #fff;
  border-top:1px solid #F7F7F7;
  border-bottom:1px solid #F7F7F7;
}
.zx_program .item {
  min-width:70rpx;
  height:100%;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  justify-content: center;
}

.zx_program .active {
  color:#286E87;
  border-bottom: 6rpx solid #50C397;
}
.list {
  /* display: flex;
  flex-wrap: wrap; */
  background: #fff;
  margin-bottom:30rpx;
  border-bottom:1px solid rgba(229,229,229,1);
}
.fold {
  max-height:270rpx;
  overflow:hidden;
}
.list view {
  padding:10rpx;
  font-size:25rpx;
  border:1px solid #e5e5e5;
  margin:10rpx;
}
.list .ac {
  border-color: #50C397;
  background: #50C397;
  color: #fff;
}
.upp {
  background: #fff;
  display: flex;
  align-items: center;
  padding:50rpx
}
.upp image {
  width:139rpx;
  height:139rpx;
  margin-right:20rpx;
}
.add-btn {
  width:139rpx;
  line-height:139rpx;
  border:1px solid rgba(220,220,220,1);
  border-radius:10rpx;
  font-size: 60rpx;
  color:#D5D5D5;
  text-align: center;
}
.emit-btn {
  width:364rpx;
  line-height:72rpx;
  background:linear-gradient(-7deg,rgba(68,179,154,1),rgba(88,204,129,1));
  border-radius:36rpx;
  font-size:25rpx;
  color: #fff;
  text-align: center;
  margin:33rpx auto;
}
.f {
  border-top: 1px solid rgba(229,229,229,1);
  height:75rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 65rpx 0 45rpx;
  background: #fff;
  font-size:25rpx;
  color:#434343;
}
.ip {
  display: flex;
  align-items: center;
  justify-content: space-around;
  background: #fff;
  padding:35rpx 0;
}
.ipi {
  display: flex;
  flex-direction: column;
  font-size:25rpx;
  color:rgba(49,49,49,1);
  align-items: center;
}
.ipi input {
  width:139rpx;
  height:64rpx;
  border:1px solid rgba(220,220,220,1);
  border-radius:8rpx;
  text-align: center;
  margin-top:26rpx;
}