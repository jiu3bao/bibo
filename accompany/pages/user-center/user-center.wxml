    <view class='qrcode'>
        
    </view>
    <view class='name'>
      <image src='{{info.head}}'></image>
      {{info.name}}
    </view>
    <view class='btn'>
      保存图片
    </view>
  </view> -->
</view>


<tabbar nowtab='{{3}}'></tabbar>