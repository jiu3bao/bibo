/* accompany/pages/my-vip.wxss */
page {
  background: #f7f7f7;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.p {
  padding:0 36rpx;
  height:100%;
  overflow-y: scroll;
  
}
.search {
  width: 100%;
  height:67rpx;
  border:2px solid rgba(222, 222, 222, 1);
  border-radius:34rpx;
  display: flex;
  align-items: center;
  margin:28rpx 0;
}
.search input {
  width: 100%;
  text-indent: 2em;
}
.search image {
  width:33rpx;
  height:33rpx;
  margin:0 36rpx;
}
.list {
  width:100%;
  height:180rpx;
  background:rgba(255,255,255,1);
  border:1px solid rgba(220,220,220,1);
  border-radius:10rpx;
  display: flex;
  align-items: center;
  margin-bottom: 20rpx;
}
.list image {
  width:98rpx;
  height:98rpx;
  border-radius:50%;
  margin:0 32rpx;
}
.right {

}
.name {
  font-size:29rpx;
  color:rgba(65,122,145,1);
  font-weight:600;
  margin-bottom: 27rpx;
}
.name image {
  width: 28rpx;
  height:28rpx;
  border-radius:50%;
  margin-left:11rpx;
}
.wx {
  font-size:25rpx;
  color:rgba(49,49,49,1);
}